{
  "name": "dotsjustdots",
  "private": true,
  "version": "0.0.1",
  "description": "dots and boxes is markdown for animations experiment",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "cp-dot-and-boxes": "cp ./dist/assets/index*.js docs/static/dots-and-boxes.js",
    "build-docs": "npm run build && npm run cp-dot-and-boxes && cd docs && npm install && npm run build",
    "build-editor": "npm run build && cp ./dist/assets/index*.js editor/lib/dots-and-boxes.js",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "devDependencies": {
    "typescript": "^5.0.2",
    "vite": "^5.0.12",
    "vitest": "^0.34.4"
  }
}
